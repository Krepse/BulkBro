# BulkBro Environment Variables
# Copy this file to .env and fill in your values

# Supabase Configuration (Required)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Strava Integration (Required for Strava features)
# Only the client ID is needed here - the secret is stored in Supabase Edge Function secrets
VITE_STRAVA_CLIENT_ID=your_strava_client_id

# Admin Configuration (Optional)
# Email address that has access to admin features
VITE_ADMIN_EMAIL=admin@example.com
